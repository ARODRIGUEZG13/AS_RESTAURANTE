	<queryString>
		<![CDATA[SELECT * FROM EMPRESA.facturacion
INNER JOIN EMPRESA.PEDIDO ON empresa.facturacion.id_pedido = empresa.pedido.id_pedido
INNER JOIN EMPRESA.PEDIDO_DETALLE ON empresa.pedido.id_pedido= empresa.pedido_detalle.id_pedido
INNER JOIN EMPRESA.CLIENTE ON empresa.facturacion.nit = empresa.cliente.nit
INNER JOIN EMPRESA.MENU ON empresa.pedido_detalle.id_menu = empresa.menu.id_menu WHERE empresa.facturacion.id_factura=$P{IdFactura}]]>
	</queryString>
